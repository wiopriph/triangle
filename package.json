  "devDependencies": {
    "@babel/eslint-parser": "7.18.9",
    "@nuxtjs/eslint-config": "6.0.1",
    "@nuxtjs/eslint-module": "3.1.0",
    "@nuxtjs/stylelint-module": "4.1.0",
    "eslint": "7.32.0",
    "eslint-formatter-gitlab": "2.2.0",
