    "eslint-plugin-wdio": "7.21.0",
    "postcss": "8.4.16",
    "postcss-html": "1.5.0",
    "prettier": "2.7.1",
    "stylelint": "14.11.0",
    "stylelint-config-rational-order": "0.1.2",
